<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <filter>
        <filter-name>UserSession</filter-name>
        <filter-class>WControl.UserSessionJspControl</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>UserSession</filter-name>
        <url-pattern>*.jsp</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>Main</servlet-name>
        <servlet-class>WControl.Main</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>WControl.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>WControl.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserRegister</servlet-name>
        <servlet-class>WControl.UserRegister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewProject</servlet-name>
        <servlet-class>WControl.NewProject</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MemberInfo</servlet-name>
        <servlet-class>WControl.MemberInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProjectHome</servlet-name>
        <servlet-class>WControl.ProjectHome</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EnterProject</servlet-name>
        <servlet-class>WControl.EnterProject</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProjectListId</servlet-name>
        <servlet-class>WControl.ProjectListId</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Main</servlet-name>
        <url-pattern>/Main</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/Home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserRegister</servlet-name>
        <url-pattern>/UserRegister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewProject</servlet-name>
        <url-pattern>/NewProject</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MemberInfo</servlet-name>
        <url-pattern>/MemberInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProjectHome</servlet-name>
        <url-pattern>/ProjectHome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EnterProject</servlet-name>
        <url-pattern>/EnterProject</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProjectListId</servlet-name>
        <url-pattern>/ProjectListId</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
